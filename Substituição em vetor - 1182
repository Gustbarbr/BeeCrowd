#include <iostream>
using namespace std;
#include <iomanip>
#include <string>
#include <math.h>
#include <algorithm>
#include <vector>
#include <set>
 
int main(){

  int n, cont = 0;
  
  vector<int>v;

  for(int i = 0; i < 10; i++){
    
    cin >> n;
    
    if(n <= 0){
      v.push_back(1);
      cout << "X[" << cont << "] = " << 1 << endl;
    }
      
    else{
      v.push_back(n);  
      cout << "X[" << cont << "] = " << n << endl;
    }

    cont += 1;
  }
}
