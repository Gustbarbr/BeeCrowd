#include <iostream>
using namespace std;
#include <iomanip>

int main () {
  double A, B, C;
  double pi = 3.14159;

  cin >> A >> B >> C;
  
  cout << "TRIANGULO: " << fixed << setprecision(3) << (A*C)/2 << endl << "CIRCULO: " << fixed << setprecision(3) << pi * (C*C) << endl << "TRAPEZIO: " << fixed << setprecision(3) << ((A+B) * C) / 2 << endl << "QUADRADO: " << fixed << setprecision(3) << B*B << endl << "RETANGULO: " << fixed << setprecision(3) << A*B << endl;
}
