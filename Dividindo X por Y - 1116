#include <iostream>
using namespace std;
#include <iomanip>
 
int main(){

  float N, X, Y, resultado = 0;

  cin >> N;

  for(int i = 0; i < N; i++){
    
    cin >> X >> Y;
    
    if(Y != 0){
    
      resultado = X / Y;
      cout << resultado << endl;

    }
    else{
      cout << fixed << setprecision(1) << "divisao impossivel" << endl;
    }
  }
}
